/**
 * Get System Common Data.
 */
buildHeaderLoginStatus = function() {
    var loginStatus = pagePolicy.getCommonData(COMMON_CONSTS.COMMON_DATA_API.REQUEST.LOGIN_STATUS);
    if (loginStatus) {
        jQuery("#header .account .login_block").css("display", "none");
        jQuery("#header .account .logout_block").css("display", "inline");
    } else {
        jQuery("#header .account .login_block").css("display", "inline");
        jQuery("#header .account .logout_block").css("display", "none");
    }
}
buildHeaderCartTotalCount = function() {
    var cartTotalCount = pagePolicy.getCommonData(COMMON_CONSTS.COMMON_DATA_API.REQUEST.CART_TOTAL_COUNT);
    jQuery("#header .cart .count").text(cartTotalCount);
}
pagePolicy.addParameter(COMMON_CONSTS.PAGE_POLICY.PARAMETER_NAMES.SUCCESS_FUNCTION, buildHeaderLoginStatus);
pagePolicy.addParameter(COMMON_CONSTS.PAGE_POLICY.PARAMETER_NAMES.SUCCESS_FUNCTION, buildHeaderCartTotalCount);
</script>